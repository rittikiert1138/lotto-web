generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  user_id      Int     @id @default(autoincrement())
  phone_number String  @unique
  username     String  @unique
  credit       Int?
  email        String?
}
